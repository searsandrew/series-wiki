{
  "name": "searsandrew/series-wiki",
  "description": "Series-agnostic wiki backend toolkit for Laravel (spoiler gates, templates, timelines, link suggestions).",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "illuminate/support": "^11.0|^12.0",
    "illuminate/database": "^11.0|^12.0",
    "illuminate/validation": "^11.0|^12.0"
  },
  "require-dev": {
    "laravel/framework": "^11.0|^12.0",
    "orchestra/testbench": "^9.0|^10.0",
    "pestphp/pest": "^3.0",
    "pestphp/pest-plugin-laravel": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Searsandrew\\SeriesWiki\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Searsandrew\\SeriesWiki\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Searsandrew\\SeriesWiki\\SeriesWikiServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    },
    "platform": {
      "php": "8.2.0"
    }
  }
}
